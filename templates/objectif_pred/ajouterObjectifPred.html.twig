{% extends 'Back/base.html.twig' %}
{% block content %}
    <br>
    <div class="container mt -4" style="margin-right: 110px">
        <h1>Ajouter un objectif</h1>
        {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
        <div class="row">
            <div class="col-md-8"> {{ form_row(form.description) }}</div>
        </div>
        <div class="row">
            <div class="col-md-4"> {{ form_row(form.duree) }}</div>
        </div>
        {{ form_rest(form) }}
        <br>
        <button class="btn btn-primary"> Ajouter</button>
        {{ form_end(form) }}
        <br>
    </div>
{% endblock %}